{
    "ext": {
        "image": {
            "ext": ".jpg.jpeg.png.gif.tif",
            "check_folder": true,
            "match_precent": 60
        },
        "document": {
            "ext": ".doc.docx.txt.odt.pdf.xls.xlsx.ods.ppt.pptx"
        },
        "adobe": {
            "ext": ".psd.ai"
        }
    },
    "analyze": [
        {
            "track": "$HOME$/Downloads",
            "dest": [
            {
                "ext": "image",
                "dest": "$HOME$/Pictures"
            },
            {
                "ext": "document",
                "dest": "$HOME$/Documents"
            },
            {
                "ext": "adobe",
                "dest": "$HOME$/Creative Cloud Files"
            }
            ]
        }
    ]
}